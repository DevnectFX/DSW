@inherits DSW.ViewEngines.Razor.DSWRazorViewBase<dynamic>
@{
  ViewBag.Title = "메뉴등록관리";
}
<div class="span4"> <!-- 왼쪽 메뉴 트리 -->
  <ul id="treeview">
    <li><span>시스템관리</span>
    </li>
  </ul>
</div>
<div class="span8"> <!-- 오른쪽 메뉴 상세 -->
  @Model
</div>
@section body {
  <script>
    $(document).ready(function()
    {
      $("#treeview").kendoTreeView();
    });
  </script>
}
